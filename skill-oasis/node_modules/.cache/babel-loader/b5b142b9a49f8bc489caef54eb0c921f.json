AOA,gBAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,gBAAA,OAAA,CAAA,GAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAZA;AAaA,IAAA,WAbA,yBAaA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA;AACA,kBAAA,WAAA,EAAA,MAAA,CAAA,WADA;AAEA,kBAAA,IAAA,EAAA,MAAA,CAAA,IAFA;AAGA,kBAAA,YAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CAFA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAQA,gBAAA,OAAA,CAAA,GAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AAvBA;AArBA,CAAA","sourcesContent":["<template>\n  <div class=\"container-fluid\" style=\"height: 1440px\">\n    <div style=\"background-color: #79b270\">\n      <p class=\"fs-3\" style=\"color: #ffffff; text-align: center; padding: 15px\">\n        登録が完了しました。ようこそ、{{ loginUser.userName }}さん。\n      </p>\n    </div>\n    <div class=\"container\" style=\"max-width: 720px; height: 873px\">\n      <div class=\"card text-center\" style=\"padding: 0 30px\">\n        <div class=\"card-body\" style=\"padding: 1rem 3rem\">\n          <p\n            class=\"card-title fs-3 mb-5 w-100 mx-auto\"\n            style=\"border-bottom: 2.1px solid #eceeec; color: #79b270\"\n          >\n            プロフィールを作成\n          </p>\n          <div class=\"row g-3\">\n            <div class=\"col-sm\">\n              <img\n                class=\"img-fluid rounded-circle\"\n                style=\"width: 100px; height: 100px; background-color: #eceeec\"\n              />\n              <p class=\"fs-6\">画像を設定</p>\n            </div>\n            <div class=\"col-sm mt-3\">\n              <p class=\"mb-0\" style=\"text-align: left\">ユーザー名</p>\n              <input\n                v-model=\"profileName\"\n                type=\"text\"\n                class=\"form-control mb-3\"\n                style=\"background-color: #eceeec; border: none\"\n                placeholder=\"例:あんぱん食べたい人\"\n              />\n              <p class=\"mb-0\" style=\"text-align: left\">目標</p>\n              <input\n                v-model=\"goal\"\n                type=\"text\"\n                class=\"form-control\"\n                style=\"background-color: #eceeec; border: none\"\n                placeholder=\"例:大会に出れるレベルまで行きたいです\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <p class=\"card-text-left fs-5 w-50\">自己紹介</p>\n            <textarea\n              v-model=\"selfIntroduction\"\n              class=\"form-control\"\n              style=\"background-color: #eceeec; border: none\"\n              rows=\"12\"\n            ></textarea>\n          </div>\n          <div class=\"mt-3\">\n            <button\n              @click=\"makeProfile()\"\n              ref=\"/Myprofile\"\n              class=\"btn mx-auto\"\n              style=\"\n                background-color: #79b270;\n                color: #fff;\n                border: #79b270;\n                border-radius: 6px;\n                width: 100px;\n              \"\n            >\n              作成\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapGetters } from 'vuex';\nimport { Auth } from 'aws-amplify';\nimport { AmplifyEventBus } from 'aws-amplify-vue';\n\nexport default {\n  created() {\n    this.isUserSignedIn();\n    AmplifyEventBus.$on('authState', (info) => {\n      if (info === 'signedIn') {\n        this.isUserSignedIn();\n      } else {\n        this.signedIn = false;\n      }\n    });\n  },\n  computed: {\n    ...mapGetters(['loginUser']),\n  },\n  data() {\n    return {\n      profileName: '',\n      goal: '',\n      selfIntroduction: '',\n    };\n  },\n  methods: {\n    async isUserSignedIn() {\n      try {\n        await Auth.currentAuthenticatedUser().then((userObj) => {\n          this.signedIn = true;\n          console.log(userObj);\n        });\n      } catch (err) {\n        this.signedIn = false;\n        console.log(err);\n        this.$router.push('/');\n      }\n    },\n    async makeProfile() {\n      try {\n        await this.$store.commit('setUserProfile', {\n          profileName: this.profileName,\n          goal: this.goal,\n          introduction: this.selfIntroduction,\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}